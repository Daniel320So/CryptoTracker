@model IEnumerable<CryptoTracker.Models.WalletDto>
@{
    ViewBag.Title = "List";
}

<h2>Wallet Summary</h2>
<a class="a-green" href="/Wallet/New">New Wallet</a>
@foreach (var wallet in Model)
{
    <h2><a href="/Wallet/Details/@wallet.WalletId">@wallet.WalletName</a></h2>
    <h3> Balancec: $@wallet.WalletTotalValue</h3>
    <div>Description: @wallet.WalletDescription</div>
}